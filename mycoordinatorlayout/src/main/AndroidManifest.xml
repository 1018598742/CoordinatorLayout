<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mycoordinatorlayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.MyFloatingActionButton"
            android:label="@string/title_activity_my_tab_layout"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.MyTabLayout" />
        <activity android:name=".activity.MyTabLayout2" />
        <activity android:name=".activity.MyBehaviorLayout"></activity>
    </application>

</manifest>